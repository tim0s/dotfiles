syntax on
colorscheme torte
set nocompatible
set incsearch
set ignorecase
set smartcase
set number
set foldmethod=syntax
set smartindent
set tabstop=4
set backspace=2
set showmode
set ruler
set nojoinspaces
set laststatus=2
set modelines=1
set title
set runtimepath=~/.vim,$VIMRUNTIME
set viminfo='20,\"50
set history=500
set colorcolumn=80
let g:ycm_confirm_extra_conf = 0

